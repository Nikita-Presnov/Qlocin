{
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "disasexpl.associations": {
    
        "**/*.c": "${fileDirname}/build/debug/obj/${fileBasenameNoExtension}.S",
        "**/*.cpp": "${fileDirname}/build/debug/obj/${fileBasenameNoExtension}.S"
    },
    "lldb.displayFormat": "auto",
    "lldb.showDisassembly": "always",
    "lldb.dereferencePointers": true,
    "files.associations": {
        "*.py": "python",
        "*.ld": "linkerscript",
        "*.tcc": "cpp",
        "string": "cpp",
        "functional": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "ostream": "cpp",
        "sstream": "cpp",
        "streambuf": "cpp",
        "string_view": "cpp",
        "tuple": "cpp"
    }
}
